          "/bookings": {"get": {"summary": "My bookings"}, "post": {"summary": "Create booking"}},
          "/bookings/{id}": {"get": {"summary": "Get booking"}, "put": {"summary": "Update booking"}, "delete": {"summary": "Cancel booking"}}
        }
      }
