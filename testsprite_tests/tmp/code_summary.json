{
  "tech_stack": [
    "javascript",
    "node.js",
    "express",
    "mongodb",
    "mongoose",
    "react",
    "vite",
    "cloudinary",
    "razorpay",
    "nodemailer"
  ],
  "features": [
    {
      "name": "Health API",
      "description": "Health check endpoint to verify API is running",
      "files": [
        "backend/server.js"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "HomelyHub Health API", "version": "1.0.0"},
        "servers": [{"url": "http://localhost:5001/api/v1"}],
        "paths": {
          "/health": {
            "get": {
              "summary": "Health check",
              "responses": {"200": {"description": "OK"}}
            }
          }
        }
      }
    },
    {
      "name": "Auth API",
      "description": "User registration, login, profile, password mgmt",
      "files": [
        "backend/routes/auth.js",
        "backend/controllers/authController.js",
        "backend/middleware/auth.js",
        "backend/models/User.js"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "HomelyHub Auth API", "version": "1.0.0"},
        "servers": [{"url": "http://localhost:5001/api/v1"}],
        "paths": {
          "/auth/register": {"post": {"summary": "Register"}},
          "/auth/login": {"post": {"summary": "Login"}},
          "/auth/logout": {"post": {"summary": "Logout"}},
          "/auth/me": {"get": {"summary": "Get current user"}},
          "/auth/updatepassword": {"put": {"summary": "Update password"}},
          "/auth/forgotpassword": {"post": {"summary": "Forgot password"}},
          "/auth/resetpassword/{token}": {"put": {"summary": "Reset password", "parameters": [{"name": "token", "in": "path", "required": true}]}}
        }
      }
    },
    {
      "name": "Users API",
      "description": "User profile and management",
      "files": [
        "backend/routes/users.js",
        "backend/controllers/userController.js",
        "backend/models/User.js"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "HomelyHub Users API", "version": "1.0.0"},
        "servers": [{"url": "http://localhost:5001/api/v1"}],
        "paths": {
          "/users/profile": {"get": {"summary": "Get my profile"}, "put": {"summary": "Update my profile"}},
          "/users/{id}": {"get": {"summary": "Get user by id", "parameters": [{"name": "id", "in": "path", "required": true}]}}
        }
      }
    },
    {
      "name": "Properties API",
      "description": "Property list/search/detail and CRUD",
      "files": [
        "backend/routes/properties.js",
        "backend/controllers/propertyController.js",
        "backend/models/Property.js"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {"title": "HomelyHub Properties API", "version": "1.0.0"},
        "servers": [{"url": "http://localhost:5001/api/v1"}],
        "paths": {
          "/properties": {"get": {"summary": "List properties"}, "post": {"summary": "Create property"}},
          "/properties/search": {"get": {"summary": "Search properties"}},
          "/properties/{id}": {"get": {"summary": "Get property"}, "put": {"summary": "Update property"}, "delete": {"summary": "Delete property"}}
        }
      }
    },
    {
      "name": "Bookings API",
