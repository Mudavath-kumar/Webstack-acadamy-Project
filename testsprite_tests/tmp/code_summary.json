          "/reviews": {"get": {"summary": "List reviews"}, "post": {"summary": "Create review"}},
          "/reviews/{id}": {"get": {"summary": "Get review"}, "put": {"summary": "Update review"}, "delete": {"summary": "Delete review"}}
        }
